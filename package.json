{
  "name": "lizod",
  "version": "0.2.3",
  "packageManager": "pnpm@8.3.1",
  "description": "lightweight zod-like validator",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsc -p .",
    "test": "vitest --run"
  },
  "keywords": [
    "zod",
    "typescript"
  ],
  "author": "mizchi",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "peerDependencies": {
    "typescript": ">=5"
  },
  "files": [
    "lib",
    "src"
  ]
}
